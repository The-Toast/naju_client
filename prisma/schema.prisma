generator client {
  provider = "prisma-client-js"
  output   = "../src/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String   @id @default(cuid())
  kakaoId      String   @unique
  email        String
  nickname     String
  profileImage String
  createdAt    DateTime @default(now())
}
